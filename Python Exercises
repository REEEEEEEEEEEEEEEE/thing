>>> n1 = int( "%s" % a )
>>> n2 = int( "%s%s" % (a,a) )
>>> n3 = int( "%s%s%s" % (a,a,a) )
>>> print (n1+n2+n3)
615

>>> from datetime import date
>>> f_date = date(2003, 3, 19)
>>> l_date = date(2017, 10, 11)
>>> delta = l_date - f_date
>>> print(delta.days)
5320